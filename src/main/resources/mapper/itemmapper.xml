<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.hong.mapper.ItemMapper">
      <select id="getAllItem" parameterType="itemDto" resultType="HashMap">
          SELECT * FROM item
      </select>
        <select id="searchItem" parameterType="itemDto" resultType="HashMap">
            SELECT * FROM item WHERE item_name = #{item_name}
        </select>
</mapper>